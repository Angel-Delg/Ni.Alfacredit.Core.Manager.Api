{
   "version": "0.2.0",
   "configurations": [
      {
         "name": ".NET Core Launch (web)",
         "type": "coreclr",
         "request": "launch",
         "preLaunchTask": "build",
         "program": "${workspaceFolder}/src/Ni.Alfacredit.Core.Manager.Api/bin/Debug/net8.0/Ni.Alfacredit.Core.Manager.Api.dll",
         "args": [],
         "cwd": "${workspaceFolder}/Ni.Alfacredit.Core.Manager.Api",
         "stopAtEntry": false,
         "serverReadyAction": {
            "action": "openExternally",
            "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
         },
         "env": {
            "ASPNETCORE_ENVIRONMENT": "Development"
         },
         "sourceFileMap": {
            "/Views": "${workspaceFolder}/Views"
         }
      }

   ]
}